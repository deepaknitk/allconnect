{
  "name": "allconnect",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "client-install": "npm install --prefix client",
    "start": "node app.js",
    "app": "nodemon app.js",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run app\" \"npm run client\"",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm run build --prefix client"
  },
  "dependencies": {
    "concurrently": "^4.1.0",
    "cookie-parser": "~1.4.3",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "express": "~4.16.0",
    "http-errors": "~1.6.2",
    "mongoose": "^5.3.15",
    "morgan": "~1.9.0",
    "pug": "2.0.0-beta11"
  },
  "engines": {
    "node": "8.9.3"
  }
}
